        - id: 17
          description: permanent_fault
        - id: 11
          description: lka_missing_unavailable2
        - id: 9
          description: temporary_fault2
        - id: 5
          description: active
        - id: 3
          description: lka_missing_unavailable
        - id: 1
          description: standby
